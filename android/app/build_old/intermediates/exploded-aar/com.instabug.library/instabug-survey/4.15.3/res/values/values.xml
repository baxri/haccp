    <style name="InstabugSurveyDialogQuestion" parent="InstabugBaseText">
        <item name="android:textSize">@dimen/survey_question_title</item>
        <item name="android:textColor">?attr/instabug_survey_question_color</item>
        <item name="android:gravity">start</item>
        <item name="android:textStyle">normal</item>
        <item name="android:lineSpacingExtra">8dp</item>
        <item name="android:layout_marginRight">@dimen/survey_text_end_margin</item>
        <item name="android:layout_marginLeft">@dimen/instabug_dialog_container_padding</item>
        <item name="android:layout_marginTop">@dimen/survey_question_top_margin</item>
        <item name="android:background">?attr/instabug_survey_dialog_header_transition</item>
        <item name="android:ellipsize">none</item>
        <item name="android:maxLines">4</item>
    </style>
    <style name="InstabugSurveyEditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/instabug_dialog_container_padding</item>
        <item name="android:layout_marginRight">@dimen/instabug_dialog_container_padding</item>
        <item name="android:layout_marginTop">@dimen/survey_edittext_top_margin</item>
        <item name="android:background">?attr/survey_edittext_background</item>
        <item name="android:minHeight">60dp</item>
        <item name="android:gravity">start</item>
        <item name="android:hint">@string/instabug_str_hint_enter_your_answer</item>
        <item name="android:inputType">textAutoCorrect|textMultiLine</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:paddingLeft">@dimen/instabug_container_padding</item>
        <item name="android:paddingRight">@dimen/instabug_container_padding</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:textColor">?attr/instabug_survey_edittext_color</item>
        <item name="android:textSize">15sp</item>
    </style>
